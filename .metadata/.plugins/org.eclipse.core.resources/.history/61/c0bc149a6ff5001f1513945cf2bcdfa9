package com.Testng;

import static org.junit.jupiter.api.Assertions.*;

import java.util.Arrays;

import org.junit.jupiter.api.Test;

class TestingPerformance {

	@Test
	void test() {
		public int[] sortingArray(int[] arr) {
			for (int i = 0; i < 1000000; i++) {
				Arrays.sort(arr);
			}
			return arr;
		}
	}

}
